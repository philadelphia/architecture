<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="85px"
    android:id="@+id/item_product_borrow"
    android:orientation="horizontal">

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="10"
        android:gravity="center"
        android:text="@string/WordNumber"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/WorkNumber"/>

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="#cccccc" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="10"
        android:gravity="center"
        android:text="主板"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/MainBroad"/>

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="#cccccc" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="10"
        android:gravity="center"
        android:text="小板"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/LittleBroad"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0"
        android:gravity="center"
        android:text="@string/PCB_Code"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/PWB_Code"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0"
        android:gravity="center"
        android:text="@string/FormMaterialNumber"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/FormMaterialNumber"/>

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="#cccccc" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="10"
        android:gravity="center"
        android:text="@string/Line"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/Line"/>

    <TextView
        android:id="@+id/PWB_MaterialNumber"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0"
        android:gravity="center"
        android:text="@string/PWB_MaterialNumber"
        android:textColor="#333333"
        android:textSize="30px"
        />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="#cccccc" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="10"
        android:gravity="center"
        android:text="@string/Cover"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/Cover"/>

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="#cccccc" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="20"
        android:gravity="center"
        android:text="@string/PlanOnLineTime"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/PlanOnLineTime"/>

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="#cccccc" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="10"
        android:gravity="center"
        android:text="@string/Status"
        android:textColor="#333333"
        android:textSize="30px"
        android:id="@+id/ProductToolsStatus"/>
</com.zhy.autolayout.AutoLinearLayout>