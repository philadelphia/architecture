<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="wrap_content"
    android:layout_height="100px"
    android:gravity="center"
    android:background="@color/c_efefef"
    >

    <TextView
        android:id="@+id/shortList_statistics"
        style="@style/item_textstyle"
        android:layout_width="@dimen/px_240"
        android:layout_height="match_parent"
        android:text="@string/storage_pcb"

        />

    <TextView
        android:id="@+id/shortList_pcbcode"
        android:layout_width="@dimen/px_180"
        android:layout_height="match_parent"
        android:text="@string/pcb_code"
        style="@style/item_textstyle"
        />

    <TextView
        android:id="@+id/shortList_datacode"
        android:layout_width="@dimen/px_180"
        android:layout_height="match_parent"
        android:text="@string/pcb_time"
        style="@style/item_textstyle"

        />
    <TextView
        android:id="@+id/shortList_count"
        android:layout_width="@dimen/px_240"
        android:layout_height="match_parent"
        android:text="@string/pcb_count"
        style="@style/item_textstyle"

        />
    <TextView
        android:id="@+id/shortList_forprices"
        android:layout_width="@dimen/px_240"
        android:layout_height="match_parent"
        android:text="@string/pcb_price"
        style="@style/item_textstyle"

        />



</com.zhy.autolayout.AutoLinearLayout>
